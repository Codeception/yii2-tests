{
  "name": "sam-it/yii2-codeception-tests",
  "description": "Yii2 Codeception Tests",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": "dev-master",
    "yiisoft/yii2-app-advanced": "^2.0",
    "codeception/verify": "*",
    "codemix/yii2-localeurls": "^1.7"
  },
  "require-dev": {
     "codeception/codeception": "2.4.x-dev"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sam Mousa",
      "email": "sam@mousa.nl"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "bin-dir": "bin"
  },
  "autoload": {
    "classmap": [
      "vendor/yiisoft/yii2/Yii.php",
      "cases"
    ]
  },
  "scripts": {
    "test" : "codecept run"
  }
}
